#include <bits/stdc++.h>
#define M 1000000007
using namespace std;

typedef long long int ll;

int main()
{
    double n;
    cin>>n;
    double ans = 0;
    for(int i=0;i<n;i++)
    {
        double dino = i;
        ans = ans + 1/(n-dino);
    }
    cout<<fixed<<showpoint;
    cout<< setprecision(10) <<ans<<endl;

}
